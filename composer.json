{
    "name": "laravel-liberu/localisation",
    "description": "Localisation management dependency for Laravel Liberu",
    "keywords": [
        "laravel-liberu",
        "localisation"
    ],

    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",

            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/forms": "^4.0",
        "laravel-liberu/helpers": "^2.0",
        "laravel-liberu/select": "^4.0",
        "laravel-liberu/migrator": "^2.0",
        "laravel-liberu/tables": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelLiberu\\Localisation\\": "src/",
            "LaravelLiberu\\Localisation\\Database\\Factories\\": "database/factories/",
            "LaravelLiberu\\Localisation\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLiberu\\Localisation\\AppServiceProvider",
                "LaravelLiberu\\Localisation\\AuthServiceProvider"
            ],
            "aliases": {}
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0"
    }
}
